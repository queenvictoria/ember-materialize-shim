{
  "name": "ember-materialize-shim",
  "version": "0.5.1",
  "description": "A simple shim for using Materialize in Ember.js apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "ember try:testall",
    "semantic-release": "semantic-release"
  },
  "repository": "https://github.com/levanto-financial/ember-materialize-shim",
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "@mike-north/js-lib-renovate-config": "1.1.5",
    "@mike-north/js-lib-semantic-release-config": "1.0.1",
    "bower": "1.8.4",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli-dependency-checker": "3.1.0",
    "ember-cli-eslint": "5.0.0",
    "ember-cli-fastboot": "2.0.0",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-shims": "1.2.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-uglify": "2.1.0",
    "ember-cli": "3.6.1",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-disable-proxy-controllers": "1.0.2",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "2.0.0",
    "ember-material-design-icons-shim": "0.1.13",
    "ember-resolver": "5.0.1",
    "ember-source-channel-url": "1.1.0",
    "ember-source": "3.6.1",
    "ember-try": "1.1.0",
    "eslint-plugin-ember": "6.1.0",
    "eslint-plugin-node": "8.0.0",
    "husky": "1.2.1",
    "loader.js": "4.7.0",
    "semantic-release": "15.12.4",
    "sass": "1.15.2"
  },
  "keywords": [
    "ember-addon",
    "material",
    "materialdesign",
    "materialize",
    "ui",
    "form",
    "skin",
    "paper",
    "bootstrap",
    "design",
    "android"
  ],
  "dependencies": {
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "3.0.2",
    "ember-cli-babel": "7.2.0",
    "ember-cli-htmlbars": "3.0.1",
    "ember-cli-sass": "^8.0.1",
    "exists-sync": "0.1.0",
    "fastboot-transform": "0.1.3"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
